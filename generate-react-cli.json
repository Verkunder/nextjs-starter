{
    "usesTypeScript": true,
    "usesCssModule": true,
    "cssPreprocessor": "css",
    "testLibrary": "None",
    "component": {
        "default": {
            "path": "./components",
            "withStyle": false,
            "withTest": true,
            "withStory": true,
            "withIndex": true,
            "withLazy": false,
            "customTemplates": {
                "index": "./config/templates/index.ts",
                "component": "./config/templates/template-name.tsx",
                "story": "./config/templates/template-name.stories.tsx",
                "test": "./config/templates/template-name.test.tsx"
            }
        }
    },
    "usesStyledComponents": false
}
