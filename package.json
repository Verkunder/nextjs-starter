{
    "name": "nextjs-starter",
    "version": "3.1.0",
    "private": true,
    "scripts": {
        "prepare": "husky install",
        "build": "next build",
        "dev": "npm run clean && kill-port 3000 && next dev -p 3000",
        "prod": "next start",
        "type-check": "tsc --project tsconfig.json --noEmit --pretty",
        "lint": "next lint --quiet --format=pretty",
        "format": "pretty-quick --ignore-path .gitignore --pattern \"src/**/*.+(ts|js|tsx|json)\"",
        "analyze": "cross-env ANALYZE=true npm run build",
        "check-all": "run-p type-check lint format",
        "clean": "rimraf .next",
        "api-codegen": "openapi --input ./swagger.json --output ./src/api --request ./src/api/request.ts --useOptions",
        "storybook": "kill-port 6006 && storybook dev -p 6006 --quiet",
        "build-storybook": "storybook build",
        "build-tokens": "style-dictionary build --config tokens.config.json"
    },
    "dependencies": {
        "@sentry/nextjs": "7.45.0",
        "@tanstack/react-query": "4.28.0",
        "@tanstack/react-query-devtools": "4.28.0",
        "axios": "^1.3.4",
        "clsx": "^1.2.1",
        "dompurify": "3.0.1",
        "form-data": "4.0.0",
        "lodash": "4.17.21",
        "mobx": "^6.9.0",
        "mobx-react-lite": "3.4.3",
        "modern-normalize": "1.1.0",
        "nanoid": "^4.0.1",
        "next": "13.2.4",
        "next-seo": "5.15.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-query-kit": "1.4.1",
        "react-use": "17.4.0",
        "sharp": "0.32.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "13.2.4",
        "@storybook/addon-essentials": "7.0.0-rc.8",
        "@storybook/addon-interactions": "7.0.0-rc.8",
        "@storybook/addon-links": "7.0.0-rc.8",
        "@storybook/addon-mdx-gfm": "7.0.0-beta.54",
        "@storybook/nextjs": "7.0.0-rc.8",
        "@storybook/react": "7.0.0-rc.8",
        "@total-typescript/ts-reset": "0.4.2",
        "@types/dompurify": "3.0.0",
        "@types/lodash": "4.14.191",
        "@types/node": "^18.15.9",
        "@types/react": "18.0.29",
        "@types/react-dom": "^18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "commitizen": "4.3.0",
        "cors": "2.8.5",
        "cross-env": "7.0.3",
        "css-loader": "6.7.3",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "^8.36.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "13.2.4",
        "eslint-config-prettier": "^8.8.0",
        "eslint-formatter-pretty": "5.0.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-lodash": "7.4.0",
        "eslint-plugin-no-secrets": "0.8.9",
        "eslint-plugin-no-use-extend-native": "0.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-prefer-function-component": "3.1.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sonarjs": "0.19.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "eslint-plugin-xss": "0.1.12",
        "express": "4.18.2",
        "http-proxy-middleware": "2.0.6",
        "husky": "8.0.3",
        "kill-port": "^2.0.1",
        "lint-staged": "^13.2.0",
        "msw": "1.2.1",
        "msw-storybook-addon": "1.8.0",
        "npm-run-all": "4.1.5",
        "openapi-typescript-codegen": "0.23.0",
        "prettier": "^2.8.7",
        "pretty-quick": "3.1.3",
        "react-docgen-typescript": "2.2.2",
        "rimraf": "4.4.1",
        "sass": "^1.60.0",
        "sass-loader": "13.2.1",
        "storybook": "7.0.0-rc.8",
        "storybook-dark-mode": "2.1.1",
        "style-dictionary": "3.7.2",
        "style-loader": "3.3.2",
        "typescript": "5.0.2"
    },
    "engines": {
        "node": ">= 18",
        "npm": ">= 9"
    },
    "browserslist": [
        "last 2 version",
        "> 1%",
        "not dead"
    ],
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "msw": {
        "workerDirectory": "public"
    }
}
