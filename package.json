{
  "name": "nextjs-starter",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "build": "next build",
    "dev": "kill-port 3000 && next dev -p 3000",
    "prod": "next start",
    "check:ts": "tsc --project tsconfig.json --noEmit --pretty",
    "check:lint": "next lint",
    "check:format": "prettier . --write --cache --log-level=silent",
    "check:all": "run-p check:ts check:lint check:format test-run",
    "analyze": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next",
    "api-codegen": "kubb generate --config ./config/kubb.config.mjs",
    "storybook": "kill-port 6006 && storybook dev -p 6006 --quiet",
    "build-storybook": "storybook build",
    "build-tokens": "npx style-dictionary build --config ./config/tokens.config.json",
    "test": "vitest",
    "test-run": "vitest run",
    "coverage": "vitest run --coverage",
    "type-coverage": "npx typescript-coverage-report",
    "cruiser": "depcruise app --config ./config/.dependency-cruiser.js --output-type dot | dot -T svg -Grankdir=TD | tee dependency-graph.svg | depcruise-wrap-stream-in-html > ./docs.html",
    "gen": "generate-react component"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-radio-group": "1.1.3",
    "@radix-ui/react-scroll-area": "1.0.5",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-switch": "1.0.3",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-tooltip": "1.0.7",
    "@sentry/nextjs": "7.100.1",
    "@t3-oss/env-nextjs": "0.8.0",
    "@tanstack/react-query": "5.25.0",
    "@tanstack/react-query-devtools": "5.25.0",
    "@tanstack/react-query-next-experimental": "5.25.0",
    "axios": "1.6.7",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "immer": "10.0.4",
    "isomorphic-dompurify": "2.4.0",
    "lodash": "4.17.21",
    "lucide-react": "^0.358.0",
    "nanoid": "5.0.6",
    "next": "14.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.1",
    "react-use": "17.5.0",
    "sharp": "0.33.2",
    "tailwind-merge": "2.2.2",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.22.4",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@kubb/cli": "2.5.3",
    "@kubb/core": "2.5.3",
    "@kubb/swagger": "2.5.3",
    "@kubb/swagger-client": "2.5.3",
    "@kubb/swagger-faker": "2.5.3",
    "@kubb/swagger-tanstack-query": "2.5.3",
    "@kubb/swagger-ts": "2.5.3",
    "@kubb/swagger-zod": "2.5.3",
    "@next/bundle-analyzer": "14.1.0",
    "@storybook/addon-designs": "7.0.9",
    "@storybook/addon-essentials": "7.6.13",
    "@storybook/addon-interactions": "7.6.13",
    "@storybook/addon-links": "7.6.13",
    "@storybook/addon-storysource": "7.6.13",
    "@storybook/nextjs": "7.6.13",
    "@storybook/react": "7.6.13",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/dompurify": "3.0.5",
    "@types/lodash": "4.14.202",
    "@types/node": "20.11.25",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.3.1",
    "autoprefixer": "10.4.18",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "dependency-cruiser": "16.2.3",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-webpractik": "github:webpractik/eslint-config-webpractik",
    "eslint-plugin-tailwindcss": "3.15.1",
    "generate-react-cli": "8.4.0",
    "git-cz": "4.9.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "kill-port": "2.0.1",
    "lint-staged": "15.2.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "react-docgen-typescript": "2.2.2",
    "rimraf": "5.0.5",
    "storybook": "7.6.13",
    "storybook-addon-pseudo-states": "2.1.2",
    "storybook-dark-mode": "3.0.3",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.2",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.1"
  },
  "engines": {
    "node": "^20",
    "npm": "^10"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "trustedDependencies": [
    "@sentry/cli",
    "core-js-pure",
    "es5-ext"
  ]
}
