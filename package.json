{
    "name": "nextjs-starter",
    "version": "4.0.0",
    "private": true,
    "scripts": {
        "prepare": "husky install",
        "build": "next build",
        "dev": "kill-port 3000 && next dev -p 3000",
        "prod": "next start",
        "check:ts": "tsc --project tsconfig.json --noEmit --pretty",
        "check:lint": "next lint --format=pretty",
        "check:style": "stylelint 'app/**/*.module.sass' --fix --config ./config/stylelint.config.mjs",
        "check:format": "prettier . --write --cache --log-level=silent",
        "check:all": "run-p check:ts check:lint check:format test-run",
        "analyze": "cross-env ANALYZE=true npm run build",
        "clean": "rimraf .next node_modules",
        "api-codegen": "kubb generate --config ./config/kubb.config.mjs",
        "storybook": "kill-port 6006 && storybook dev -p 6006 --quiet",
        "build-storybook": "storybook build",
        "build-tokens": "style-dictionary build --config ./config/tokens.config.json",
        "test": "vitest",
        "test-run": "vitest run",
        "coverage": "vitest run --coverage",
        "type-coverage": "npx typescript-coverage-report",
        "cruiser": "depcruise app --include-only \"^app\" --config ./config/.dependency-cruiser.js --output-type dot | dot -T svg -Grankdir=TD | tee dependency-graph.svg | depcruise-wrap-stream-in-html > ./docs.html",
        "gen": "generate-react component"
    },
    "dependencies": {
        "@sentry/nextjs": "7.98.0",
        "@t3-oss/env-nextjs": "0.8.0",
        "@tanstack/react-query": "5.17.19",
        "@tanstack/react-query-devtools": "5.17.21",
        "@tanstack/react-query-next-experimental": "5.17.19",
        "axios": "1.6.7",
        "clsx": "2.1.0",
        "immer": "10.0.3",
        "isomorphic-dompurify": "2.3.0",
        "lodash": "4.17.21",
        "modern-normalize": "2.0.0",
        "nanoid": "5.0.4",
        "next": "14.1.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-use": "17.5.0",
        "sharp": "0.33.2",
        "zod": "3.22.4",
        "zustand": "4.5.0"
    },
    "devDependencies": {
        "@kubb/cli": "2.5.2",
        "@kubb/core": "2.5.2",
        "@kubb/swagger": "2.5.2",
        "@kubb/swagger-client": "2.5.2",
        "@kubb/swagger-faker": "2.5.2",
        "@kubb/swagger-tanstack-query": "2.5.2",
        "@kubb/swagger-ts": "2.5.2",
        "@kubb/swagger-zod": "2.5.2",
        "@next/bundle-analyzer": "14.1.0",
        "@storybook/addon-essentials": "7.6.10",
        "@storybook/addon-interactions": "7.6.10",
        "@storybook/addon-links": "7.6.10",
        "@storybook/nextjs": "7.6.10",
        "@storybook/react": "7.6.10",
        "@testing-library/react": "14.1.2",
        "@testing-library/user-event": "14.5.2",
        "@total-typescript/ts-reset": "0.5.1",
        "@types/dompurify": "3.0.5",
        "@types/lodash": "4.14.202",
        "@types/node": "20.11.8",
        "@types/react": "18.2.48",
        "@types/react-dom": "18.2.18",
        "@typescript-eslint/eslint-plugin": "6.19.1",
        "@typescript-eslint/parser": "6.19.1",
        "@vitejs/plugin-react": "4.2.1",
        "@vitest/coverage-v8": "1.2.2",
        "commitizen": "4.3.0",
        "cors": "2.8.5",
        "cross-env": "7.0.3",
        "css-loader": "6.9.1",
        "cz-conventional-changelog": "3.3.0",
        "dependency-cruiser": "16.1.0",
        "eslint": "8.56.0",
        "eslint-config-next": "14.1.0",
        "eslint-config-webpractik": "github:webpractik/eslint-config-webpractik",
        "eslint-formatter-pretty": "5.0.0",
        "generate-react-cli": "8.4.0",
        "git-cz": "4.9.0",
        "husky": "9.0.6",
        "jsdom": "24.0.0",
        "kill-port": "2.0.1",
        "lint-staged": "15.2.0",
        "npm-run-all": "4.1.5",
        "postcss-sass": "0.5.0",
        "prettier": "3.2.4",
        "react-docgen-typescript": "2.2.2",
        "rimraf": "5.0.5",
        "sass": "1.70.0",
        "sass-loader": "14.0.0",
        "storybook": "7.6.10",
        "storybook-dark-mode": "3.0.3",
        "style-dictionary": "3.9.2",
        "style-loader": "3.3.4",
        "stylelint": "16.2.0",
        "stylelint-config-clean-order": "5.4.0",
        "stylelint-config-css-modules": "4.4.0",
        "stylelint-config-sass-guidelines": "11.0.0",
        "stylelint-config-standard-scss": "13.0.0",
        "typescript": "5.3.3",
        "vite-tsconfig-paths": "4.3.1",
        "vitest": "1.2.2"
    },
    "engines": {
        "node": "^20",
        "npm": "^10"
    },
    "config": {
        "commitizen": {
            "path": "git-cz"
        }
    }
}
