{
    "name": "nextjs-starter",
    "version": "4.0.0",
    "private": true,
    "scripts": {
        "prepare": "husky install",
        "build": "next build",
        "dev": "kill-port 3000 && next dev -p 3000",
        "prod": "next start",
        "type-check": "tsc --project tsconfig.json --noEmit --pretty",
        "lint": "next lint",
        "stylelint": "stylelint '**/*.module.sass' --fix --config ./config/stylelint.config.mjs",
        "format": "prettier . --write --cache --log-level=silent",
        "analyze": "cross-env ANALYZE=true npm run build",
        "check-all": "run-p type-check lint format test-run",
        "clean": "rimraf .next",
        "api-codegen": "openapi --input ./swagger.json --output ./src/api --request ./src/api/request.ts --useOptions",
        "storybook": "kill-port 6006 && storybook dev -p 6006 --quiet",
        "build-storybook": "storybook build",
        "build-tokens": "style-dictionary build --config ./config/tokens.config.json",
        "test": "vitest",
        "test-run": "vitest run",
        "coverage": "vitest run --coverage",
        "type-coverage": "npx typescript-coverage-report",
        "cruiser": "depcruise src --include-only \"^src/app\" --config ./config/.dependency-cruiser.js --output-type dot | dot -T svg -Grankdir=TD | tee dependency-graph.svg | depcruise-wrap-stream-in-html > ./docs.html",
        "gen": "generate-react component"
    },
    "dependencies": {
        "@sentry/nextjs": "7.77.0",
        "@t3-oss/env-nextjs": "0.7.1",
        "@tanstack/react-query": "5.7.1",
        "@tanstack/react-query-devtools": "5.7.1",
        "axios": "1.6.0",
        "clsx": "2.0.0",
        "isomorphic-dompurify": "1.9.0",
        "lodash": "4.17.21",
        "mobx": "6.10.2",
        "mobx-react-lite": "4.0.5",
        "modern-normalize": "2.0.0",
        "nanoid": "5.0.2",
        "next": "14.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-use": "17.4.0",
        "sharp": "0.32.6",
        "zod": "3.22.4"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "14.0.1",
        "@storybook/addon-essentials": "7.5.2",
        "@storybook/addon-interactions": "7.5.2",
        "@storybook/addon-links": "7.5.2",
        "@storybook/nextjs": "7.5.2",
        "@storybook/react": "7.5.2",
        "@testing-library/react": "14.0.0",
        "@testing-library/user-event": "14.5.1",
        "@total-typescript/ts-reset": "0.5.1",
        "@types/dompurify": "3.0.4",
        "@types/lodash": "4.14.200",
        "@types/node": "20.8.10",
        "@types/react": "18.2.35",
        "@types/react-dom": "18.2.14",
        "@typescript-eslint/eslint-plugin": "6.9.1",
        "@typescript-eslint/parser": "6.9.1",
        "@vitejs/plugin-react": "4.1.1",
        "@vitest/coverage-v8": "0.34.6",
        "commitizen": "4.3.0",
        "cors": "2.8.5",
        "cross-env": "7.0.3",
        "css-loader": "6.8.1",
        "cz-conventional-changelog": "3.3.0",
        "dependency-cruiser": "15.1.1",
        "eslint": "8.53.0",
        "eslint-config-next": "14.0.1",
        "eslint-config-webpractik": "github:webpractik/eslint-config-webpractik",
        "generate-react-cli": "8.3.0",
        "husky": "8.0.3",
        "jsdom": "22.1.0",
        "kill-port": "2.0.1",
        "lint-staged": "15.0.2",
        "npm-run-all": "4.1.5",
        "postcss-sass": "0.5.0",
        "prettier": "3.0.3",
        "react-docgen-typescript": "2.2.2",
        "rimraf": "5.0.5",
        "sass": "1.69.5",
        "sass-loader": "13.3.2",
        "storybook": "7.5.2",
        "storybook-dark-mode": "3.0.1",
        "style-dictionary": "3.9.0",
        "style-loader": "3.3.3",
        "stylelint": "15.11.0",
        "stylelint-config-clean-order": "5.2.0",
        "stylelint-config-css-modules": "4.3.0",
        "stylelint-config-sass-guidelines": "10.0.0",
        "stylelint-config-standard-scss": "11.1.0",
        "typescript": "5.2.2",
        "vite-tsconfig-paths": "4.2.1",
        "vitest": "0.34.6"
    },
    "engines": {
        "node": "^20",
        "npm": "^10"
    },
    "browserslist": {
        "development": [
            "last 2 chrome versions",
            "last 2 firefox versions"
        ],
        "production": [
            ">1%",
            "last 4 versions",
            "Firefox ESR",
            "not ie < 11"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
